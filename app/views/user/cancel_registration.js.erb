<%= render 'application/update_flashes' %>

<%- if flash[:alert].blank? %>
  <%- if @cancel_link %>
    $("#register_event_<%= @registration.event.id %>").replaceWith("<%=j render('event/actions/schedule_general', event: @event) %>");
  <% end %>

  $("#registration_<%= @registration.id %>").fadeOut();
<% end %>
